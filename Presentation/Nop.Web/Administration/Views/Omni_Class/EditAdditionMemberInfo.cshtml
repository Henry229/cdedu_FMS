@model Nop.Admin.Models.Omni_Class.AdditionMemberInfoModel
@{
    //page title
    ViewBag.Title = T("Admin.Omni_Class.EditAdditionMemberInfoDetails").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-sales.png")" alt="" />
            @T("Admin.Omni_Class.EditAdditionMemberInfoDetails") @Html.ActionLink("(" + T("Admin.Omni_Class.AdditionalInfo.BackToList") + ")", "ClassList")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
            <span id="class-delete" class="k-button">@T("Admin.Common.Delete")</span>
        </div>
    </div>
    @Html.Partial("_AdditionMemberInfo", Model)
}
